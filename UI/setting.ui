}



/*下拉框的样式*/
QComboBox QAbstractItemView 
{
    outline: 0px solid gray;  /*取消选中虚线*/
    border: 0px solid rgb(31,156,220);  
    color: #ffffff;
    background-color: #222222;   
    selection-background-color: #a6c100;   
	border-radius:14px;

}
 /*选中每一项高度*/
